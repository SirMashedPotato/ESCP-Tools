<?xml version="1.0" encoding="utf-8" ?>
<Defs>

    <!--__________ Smelting ore to ingots __________-->

    <!-- Adamantite -->

  <RecipeDef>
    <defName>ESCP_Smelt_Adamantite</defName>
    <label>smelt admantite from ore</label>
    <description>Use heat and strong force to extract the purist of material from raw ore.</description>
    <jobString>Smelting ingots from ore.</jobString>
    <workAmount>30</workAmount>
    <workSpeedStat>SmeltingSpeed</workSpeedStat>
    <effectWorking>Smelt</effectWorking>
    <soundWorking>Recipe_Smelt</soundWorking>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>ESCP_Raw_Adamantite_Ore</li>
          </thingDefs>
        </filter>
        <count>3</count>
      </li>
    </ingredients>
    <products>
      <ESCP_Resource_Adamantite_Ingot>1</ESCP_Resource_Adamantite_Ingot>
    </products>
    <fixedIngredientFilter>
      <thingDefs>
        <li>ESCP_Raw_Adamantite_Ore</li>
      </thingDefs>
    </fixedIngredientFilter>
  </RecipeDef>

  <RecipeDef>
    <defName>ESCP_Smelt_Adamantite_Bulk</defName>
    <label>Smelt adamantite from ore in bulk</label>
